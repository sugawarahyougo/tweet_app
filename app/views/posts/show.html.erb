<div class="main posts-show">
  <div class="container">
    <div class="posts-show-item">
      <p>
        <!-- @postのcontentカラムのデータを表示してください -->
        <%= @post.content%>
      </p>
      <!-- ここに指定されたHTMLを貼り付けてください -->
      <div class="post-time">
        <!-- @postのcreated_atカラムのデータを表示してください -->
        <%= link_to("編集", "/posts/#{@post.id}/edit") %>
      </div>
    </div>
  </div>
</div>